%!PS
<< /PageSize [595 842] >> setpagedevice

/pagewidth 5 80 mul def
/pageheight 15 72 mul def

/box {w 0 rlineto 0 w rlineto -1 w mul 0 rlineto closepath} def

/cm {28.346 mul} def
/w {10.8 cm} def
/ww {w 1.5 mul} def

/marginxoffset -300 def
/marginyoffset -370 def

/Phosphate-Solid findfont
110 scalefont
setfont

/xindent {202} def
/yindent {700} def
/margin {25} def

/oshow %stack: (string)
{ true charpath stroke } def


/showallfonts {
    /x 72 def
    /y 738 def
    /fs 10 def
    /Helvetica fs selectfont
    (*) {x y moveto show /y y fs sub def y 36 lt {showpage /y 738 def}if} 256 string /Font resourceforall
    showpage
} def

/word {
    /x 1
%    /letter (J) def
    /dice  {rand 100 mod
            99
            lt}  def

    /size {y 0.17 mul} def

    /jitter {dice {0} {4} ifelse} def

    /y'   {y marginyoffset add} def
    /x'   {x marginxoffset add} def

    /y''  {y' growth mul} def%{y' jitter add} def
    /x''  x' def%{x' jitter add} def

    /yinv {10 y sub} def
    /g {0.0004 y mul} def
    /g' {1.11 g sub} def
    g' setgray

    %/AvenirNext-Heavy findfont size scalefont setfont
    %/Futura-Bold findfont size scalefont setfont
    /Phosphate-Solid findfont size scalefont setfont

    /target {dice {(J)} {(J)} ifelse} def
    /yz {y 0.1 mul } def
    %/xz' {0.5 20 y sub mul xz add} def

    /pop {y direction} def

    /xz' {0.5  pop  mul xz add} def

    1.0 scale
%    newpath
    xz' y'' moveto
    0.0 0 translate
    angle rotate
    letter
    oshow
}  def


/direction { 20 exch sub}  def

/it 4.8 def
/growth 1 def

%         startval inc    final     procedure
/wordline {w       ww     pagewidth  {
               /x exch def word
           } for} def
/page     {w       it    pageheight {
               /y exch def /x
               wordline
           } for} def


